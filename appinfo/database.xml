<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>utf8</charset>
	<table>
		<name>*dbprefix*firebase_token</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<notnull>true</notnull>
				<autoincrement>true</autoincrement>
				<unsigned>true</unsigned>
				<primary>true</primary>
				<length>8</length>
			</field>
			<field>
				<name>user_id</name>
				<type>text</type>
				<length>200</length>
				<default/>
				<notnull>true</notnull>
			</field>
			<field>
				<name>token</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>260</length>
				<default/>
			</field>
			<field>
				<name>resource</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>260</length>
				<default/>
			</field>
			<field>
				<name>locale</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>8</length>
				<default>EN-en</default>
			</field>
			<field>
				<name>last_used</name>
				<type>integer</type>
				<unsigned>true</unsigned>
				<length>16</length>
			</field>
			<field>
				<name>device_type</name>
				<type>integer</type>
				<unsigned>true</unsigned>
				<length>8</length>
			</field>
			 <index>
                <name>user_id_index</name>
                <unique>true</unique>
                <field>
                    <name>user_id</name>
                    <sorting>ascending</sorting>
                </field>
            </index>
		</declaration>
	</table>
	<table>
		<name>*dbprefix*firebase_configuration</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<notnull>false</notnull>
				<length>8</length>
				<primary>true</primary>
				<autoincrement>1</autoincrement>
			</field>
			<field>
				<name>app_name</name>
				<type>text</type>
				<notnull>false</notnull>
				<length>200</length>
				<default>firebaseApp</default>
			</field>
			<field>
				<name>server_key</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>200</length>
				<default/>
			</field>
		</declaration>
	</table>
	<table>
		<name>*dbprefix*firebase_saved_message</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<notnull>false</notnull>
				<length>8</length>
				<primary>true</primary>
				<autoincrement>1</autoincrement>
			</field>
			<field>
				<name>timestamp</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>8</length>
			</field>
			<field>
				<name>user_id</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>200</length>
				<default>firebaseApp</default>
			</field>
			<field>
				<name>title</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>200</length>
				<default/>
			</field>
			<field>
				<name>message</name>
				<type>text</type>
				<notnull>true</notnull>
				<length>1024</length>
				<default/>
			</field>
			<field>
				<name>custom_data</name>
				<type>clob</type>
				<notnull>true</notnull>
				<length>4096</length>
				<default/>
			</field>
		</declaration>
	</table>
	<table>
		<name>*dbprefix*firebase_error_log</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<notnull>false</notnull>
				<length>8</length>
				<primary>true</primary>
				<autoincrement>1</autoincrement>
			</field>
			<field>
				<name>timestamp</name>
				<type>integer</type>
				<default>0</default>
				<notnull>true</notnull>
				<length>8</length>
			</field>
			<field>
				<name>custom_data</name>
				<type>clob</type>
				<notnull>true</notnull>
				<length>4096</length>
				<default/>
			</field>
		</declaration>
	</table>
</database>
